{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:@wordpress/eslint-plugin/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off",
        "max-len": [
            "error",
            {
                "code": 180
            }
        ]
    }
}