{
	"name": "david-ev-challenge-api-based-plugin",
	"version": "1.0.0",
	"description": "A custom WordPress plugin for passing test task",
	"main": "index.js",
	"scripts": {
		"prod": "cross-env webpack --config webpack.config.prod.ts",
		"preprod": "cross-env webpack --config webpack.config.dev.ts --no-watch",
		"dev": "cross-env webpack --config webpack.config.dev.ts",
		"phpcs": "vendor/bin/phpcs -s . --standard=phpcs.xml --report=full",
		"phpcbf": "vendor/bin/phpcbf -p . --standard=phpcs.xml --report=full",
		"lint:css": "stylelint ./assets/src/styles/**/* --config ./.stylelintrc --max-warnings 0",
		"lint:js": "eslint ./assets/src/scripts/**/* --config ./.eslintrc --max-warnings 0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/outcomer/awesomemotive-api-plugin.git"
	},
	"keywords": [],
	"author": "David Evdoshchenko",
	"bugs": {
		"url": "https://github.com/outcomer/awesomemotive-api-plugin/issues"
	},
	"homepage": "https://github.com/outcomer/awesomemotive-api-plugin#readme",
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"@babel/core": "^7.21.0",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@types/browser-sync-webpack-plugin": "^2.2.2",
		"@types/node": "^18.0.5",
		"@types/webpack": "^5.28.0",
		"@wordpress/eslint-plugin": "^12.7.0",
		"autoprefixer": "^10.4.8",
		"babel-loader": "^9.1.2",
		"browser-sync": "^2.27.10",
		"browser-sync-webpack-plugin": "^2.3.0",
		"copy-webpack-plugin": "^11.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.1",
		"css-minimizer-webpack-plugin": "^4.0.0",
		"eslint-webpack-plugin": "^3.2.0",
		"image-minimizer-webpack-plugin": "^3.2.3",
		"imagemin": "^8.0.1",
		"imagemin-gifsicle": "^7.0.0",
		"imagemin-mozjpeg": "^10.0.0",
		"imagemin-pngquant": "^9.0.2",
		"imagemin-svgo": "^10.0.1",
		"mini-css-extract-plugin": "^2.6.0",
		"postcss": "^8.4.16",
		"postcss-import": "^14.1.0",
		"postcss-loader": "^7.0.1",
		"prettier": "^2.6.1",
		"sass": "^1.56.1",
		"sass-loader": "^13.2.0",
		"stylelint": "^14.15.0",
		"stylelint-config-standard-scss": "^6.1.0",
		"stylelint-order": "^5.0.0",
		"stylelint-scss": "^4.3.0",
		"stylelint-webpack-plugin": "^3.3.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4",
		"webpack": "^5.75.0",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.9.3",
		"webpack-merge": "^5.8.0"
	},
	"dependencies": {
		"npm": "^8.3.0"
	}
}
